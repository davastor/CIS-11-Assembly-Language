ExitProcess PROTO
fibGenerator PROTO

.data
fibArray QWORD 1, 2, 3, 4, 5, 6


.code
main proc
	mov rcx, 47
	mov rsi, 0
	mov fibArray[rsi], 1
	mov fibArray[rsi+8], 1
	call fibGenerator

fibGenerator proc
	L1: 
		mov rax, 0
		mov rax, fibArray[rsi]
		add rax, fibArray[rsi+8]
		mov fibArray[rsi+16], rax
		add rsi, 8
		LOOP L1
	
	ret
fibGenerator endp	



call ExitProcess
main endp
end
